{
    "name": "theahmadzai/immortal-framework",
    "description": "Immortal Framework (Library) http://www.codeinvoker.com",
    "type": "library",
    "keywords": ["immortal", "framework"],
    "homepage": "https://github.com/theahmadzai/immortal-framework",
    "license": "MIT",
    "authors": [{
        "name": "Javed Ahmadzai",
        "email": "theahmadzai@hotmail.com",
        "homepage": "http://www.codeinvoker.com",
        "role": "Developer"
    }],
    "support": {
        "email": "theahmadzai@hotmail.com",
        "issues": "https://github.com/theahmadzai/immortal-framework/issues",
        "source": "https://github.com/theahmadzai/immortal-framework"
    },
    "require": {
        "php": ">=5.6.0",
        "monolog/monolog": "@stable",
        "nikic/fast-route": "@stable",
        "psr/cache": "@stable",
        "psr/container": "@stable",
        "psr/http-message": "@stable",
        "psr/link": "@stable",
        "psr/log": "@stable",
        "psr/simple-cache": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Immortal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Immortal\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": ["@phpunit", "@phpcs"],
        "phpunit": "phpunit",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}